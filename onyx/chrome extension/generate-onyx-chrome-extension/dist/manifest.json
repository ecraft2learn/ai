{
  "key": "",
  "name": "Onyx Nail Analyser",
  "version": "0.0.0",
  "description": "Classify images of nails in Chrome.",
  "permissions": [
    "<all_urls>",
    "activeTab",
    "contextMenus",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["src/background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["src/content.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
   "128": "images/get_started128.png"
  }
}
